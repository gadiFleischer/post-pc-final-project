<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tripplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TripPlanner">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewTrip" />
        <activity android:name=".EditMap" />
        <activity android:name=".NewEvent" />
        <activity android:name=".EditEvent" />
        <activity android:name=".MyTrips" />
        <activity android:name=".TripDetails" />
        <activity android:name=".MainMapActivity" />
    </application>

</manifest>